                                        <td>@Html.DisplayFor(m => tcu.Municipio)</td>
                                        <td>@Html.DisplayFor(m => tcu.UF)</td>
                                        <td>@Html.DisplayFor(m => tcu.Deliberacao)</td>
                                        <td>@(tcu.DataJulgado.ToString("dd/MM/yyyy") ?? "-")</td>


