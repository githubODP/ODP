@model ODP.Web.UI.Models.Consultas.ResultadoConsulta.ResultadoDTO

@{
    ViewData["Title"] = "Consulta CNPJ";
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>ConsultaCNPJ</title>
</head>

<body>
    <div class="row">
        <div class="card-panel">

            <div class="container-fluid">
                <fieldset>
                    <legend>Contratos</legend>

                    @if (Model?.BuscaCompras?.Contrato != null && Model.BuscaCompras.Contrato.Count != 0)
                    {
                        <table class="table table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>Órgão</th>
                                    <th>Status</th>
                                    <th>Protocolo</th>
                                    <th>Valor Atual</th>
                                    <th>Valor Original</th>
                                    <th>CNPJ</th>
                                    <th>Data Início</th>
                                    <th>Data Fim</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var contrato in Model.BuscaCompras.Contrato)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => contrato.OrgaoGestor)</td>
                                        <td>@Html.DisplayFor(m => contrato.StatusContrato)</td>
                                        <td>@Html.DisplayFor(m => contrato.Protocolo)</td>
                                        <td>@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", contrato.VlrTotalAtual)</td>
                                        <td>@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", contrato.VlrTotalOriginal)</td>
                                        <td>@Html.DisplayFor(m => contrato.CNPJ)</td>
                                        <td>@(contrato.DTInicioVigencia?.ToString("dd/MM/yyyy") ?? "-")</td>
                                        <td>@(contrato.DTFimVigencia?.ToString("dd/MM/yyyy") ?? "-")</td>
                                    </tr>
                                }
                            </tbody>
                        </table>

                    }
                    else
                    {
                        <p>Nenhum Contrato encontrado.</p>
                    }


                </fieldset>

                <fieldset>
                    <legend>Dispensas</legend>
                    @if (Model?.BuscaCompras?.Dispensa != null && Model.BuscaCompras.Dispensa.Count != 0)
                    {
                        <table class="table table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>Órgão</th>
                                    <th>CNPJ</th>
                                    <th>Protocolo</th>
                                    <th>Valor Dispensa</th>
                                    <th>Valor Item</th>
                                    <th>Qtde Item</th>
                                    <th>Modalidade</th>
                                    <th>Data Dispensa</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var dispensa in Model.BuscaCompras.Dispensa)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => dispensa.Orgao)</td>
                                        <td>@Html.DisplayFor(m => dispensa.CNPJ)</td>
                                        <td>@Html.DisplayFor(m => dispensa.Protocolo)</td>
                                        <td>@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", dispensa.ValorDispensa)</td>
                                        <td>@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", dispensa.ValorItem)</td>
                                        <td>@Html.DisplayFor(m => dispensa.QtdItem)</td>
                                        <td>@Html.DisplayFor(m => dispensa.Modalidade)</td>
                                        <td>@(dispensa.DataDispensa?.ToString("dd/MM/yyyy") ?? "-")</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhuma Dispensa encontrada.</p>
                    }

                </fieldset>


                <fieldset>
                    <legend>Licitações</legend>
                    @if (Model?.BuscaCompras?.Licitacao != null && Model.BuscaCompras.Licitacao.Count != 0)
                    {
                        <table class="table table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>Ano</th>
                                    <th>Órgão</th>
                                    <th>CNPJ</th>
                                    <th>Protocolo</th>
                                    <th>Valor Homologado</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var dispensa in Model.BuscaCompras.Licitacao)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => dispensa.Ano)</td>
                                        <td>@Html.DisplayFor(m => dispensa.Orgao)</td>
                                        <td>@Html.DisplayFor(m => dispensa.CNPJ)</td>
                                        <td>@Html.DisplayFor(m => dispensa.Protocolo)</td>
                                        <td id="coluna">@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", dispensa.ValorHomologado)</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhuma Licitação encontrada.</p>
                    }

                </fieldset>




                <fieldset>
                    <legend>Dividas FGTS</legend>
                    @if (Model?.BuscaDividas?.DividaFGTS != null && Model.BuscaDividas.DividaFGTS.Count != 0)
                    {
                        <table class="table table-striped table-hover">

                            <thead>
                                <tr>
                                    <th>Tipo Pessoa</th>
                                    <th>Devedor</th>
                                    <th>CNPJ</th>
                                    <th>UF</th>
                                    <th>Valor</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var divida in Model.BuscaDividas.DividaFGTS)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => divida.TipoPessoa)</td>
                                        <td>@Html.DisplayFor(m => divida.NomeDevedor)</td>
                                        <td>@Html.DisplayFor(m => divida.CNPJ)</td>
                                        <td>@Html.DisplayFor(m => divida.UFUnidadeResponsavel)</td>
                                        <td>@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", divida.ValorConsolidado)</td>

                                    </tr>
                                }


                            </tbody>
                        </table>

                    }
                    else
                    {
                        <p>Nenhuma Divida encontrado.</p>
                    }

                </fieldset>

                <fieldset>
                    <legend>Dividas Previdenciárias</legend>
                    @if (Model?.BuscaDividas?.DividaPrevidenciaria != null && Model.BuscaDividas.DividaPrevidenciaria.Count != 0)
                    {
                        <table class="table table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>Tipo Pessoa</th>
                                    <th>Devedor</th>
                                    <th>CNPJ</th>
                                    <th>Data Inscrição</th>
                                    <th>Valor</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var divida in Model.BuscaDividas.DividaPrevidenciaria)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => divida.TipoPessoa)</td>
                                        <td>@Html.DisplayFor(m => divida.NomeDevedor)</td>
                                        <td>@Html.DisplayFor(m => divida.CNPJ)</td>
                                        <td>@(divida.DataInscricao.ToString("dd/MM/yyyy") ?? "-")</td>
                                        <td>@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", divida.ValorConsolidado)</td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhuma Divida encontrada.</p>
                    }

                </fieldset>

                <fieldset>
                    <legend>Dividas Não Previdenciárias</legend>
                    @if (Model?.BuscaDividas?.DividaNaoPrevidenciaria != null && Model.BuscaDividas.DividaNaoPrevidenciaria.Count != 0)
                    {
                        <table class="table table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>Tipo Pessoa</th>
                                    <th>Devedor</th>
                                    <th>CNPJ</th>
                                    <th>Data Inscrição</th>
                                    <th>Valor</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var divida in Model.BuscaDividas.DividaNaoPrevidenciaria)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => divida.TipoPessoa)</td>
                                        <td>@Html.DisplayFor(m => divida.NomeDevedor)</td>
                                        <td>@Html.DisplayFor(m => divida.CNPJ)</td>
                                        <td>@(divida.DataInscricao.ToString("dd/MM/yyyy") ?? "-")</td>
                                        <td>@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", divida.ValorConsolidado)</td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhuma Divida encontrada.</p>
                    }

                </fieldset>


                <fieldset>
                    <legend>Junta Comercial do Estado do Paraná</legend>

                    @if (Model?.BuscaFazenda?.Jucepar != null && Model.BuscaFazenda.Jucepar.Count != 0)
                    {
                        <table class="table table-striped table-hover">

                            <thead>
                                <tr>
                                    <th>Razão Social</th>
                                    <th>CNPJ</th>
                                    <th>Nomes Sócios</th>
                                    <th>CPF</th>
                                    <th>Situação</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var junta in Model.BuscaFazenda.Jucepar)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => junta.RazaoSocial)</td>
                                        <td>@Html.DisplayFor(m => junta.CNPJ)</td>
                                        <td>@Html.DisplayFor(m => junta.NomesSocio)</td>
                                        <td>@Html.DisplayFor(m => junta.CPF)</td>
                                        <td>@Html.DisplayFor(m => junta.Situacao)</td>

                                    </tr>
                                }


                            </tbody>
                        </table>

                    }
                    else
                    {
                        <p>Nenhuma Empresa encontrado.</p>
                    }
                </fieldset>


                <fieldset>
                    <legend>Notas Fiscais Eletrônicas</legend>
                    @if (Model?.BuscaFazenda?.NFEletronica != null && Model.BuscaFazenda.NFEletronica.Count != 0)
                    {
                        <table class="table table-striped table-hover">
                            <thead>

                                <tr>

                                    <th>Emitente</th>
                                    <th>CNPJ</th>
                                    <th>Numero NF</th>
                                    <th>Destinatário</th>
                                    <th>CNPJDestinatario</th>
                                    <th>Valor NF</th>
                                    <th>Data Emissão</th>
                                    <th>Ano</th>

                                </tr>


                            </thead>
                            <tbody>
                                @foreach (var nota in Model.BuscaFazenda.NFEletronica)
                                {
                                    <tr><td>@Html.DisplayFor(m => nota.Emitente)</td></tr>
                                    <tr><td>@Html.DisplayFor(m => nota.CNPJ)</td></tr>
                                    <tr><td>@Html.DisplayFor(m => nota.NumeroNF)</td></tr>
                                    <tr><td>@Html.DisplayFor(m => nota.Destinatario)</td></tr>
                                    <tr><td>@Html.DisplayFor(m => nota.CNPJDestinatario)</td></tr>
                                    <tr><td>@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", nota.ValorNotaFiscal)</td></tr>
                                    <tr><td>@Html.DisplayFor(m => nota.DataEmissao)</td></tr>
                                    <tr><td>@Html.DisplayFor(m => nota.Ano)</td></tr>
                                }

                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhuma Nota Fiscal encontrada.</p>
                    }
                </fieldset>


                <fieldset>
                    <legend>Notas Fiscais Eletrônicas Federais</legend>

                    @if (Model?.BuscaFazenda?.NFEletronicaFederal != null && Model.BuscaFazenda.NFEletronicaFederal.Count != 0)
                    {
                        <table class="table table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>Emitente</th>
                                    <th>CNPJ</th>
                                    <th>Numero NF</th>
                                    <th>Destinatário</th>
                                    <th>CNPJ Destinatário</th>
                                    <th>Valor NF</th>
                                    <th>Chave de Acesso</th>

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var nota in Model.BuscaFazenda.NFEletronicaFederal)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => nota.Emitente)</td>
                                        <td>@Html.DisplayFor(m => nota.CNPJ)</td>
                                        <td>@Html.DisplayFor(m => nota.NumeroNF)</td>
                                        <td>@Html.DisplayFor(m => nota.Destinatario)</td>
                                        <td>@Html.DisplayFor(m => nota.CNPJDestinatario)</td>
                                        <td>@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", nota.ValorNF)</td>
                                        <td>@Html.DisplayFor(m => nota.ChaveAcesso)</td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhuma Nota Fiscal encontrada.</p>
                    }

                </fieldset>


                <fieldset>
                    <legend>Infração Ambiental</legend>
                    @if (Model?.BuscaEstadual?.Ambiental != null && Model.BuscaEstadual.Ambiental.Count != 0)
                    {
                        <table class="table table-striped table-hover">

                            <thead>
                                <tr>
                                    <th>Infrator</th>
                                    <th>CNPJ</th>
                                    <th>Situação</th>
                                    <th>Ano Infração</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var ambiental in Model.BuscaEstadual.Ambiental)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => ambiental.Infrator)</td>
                                        <td>@Html.DisplayFor(m => ambiental.CNPJ)</td>
                                        <td>@Html.DisplayFor(m => ambiental.Situacao)</td>
                                        <td>@Html.DisplayFor(m => ambiental.AnoInfracao)</td>


                                    </tr>
                                }


                            </tbody>
                        </table>

                    }
                    else
                    {
                        <p>Nenhuma Infração encontrado.</p>
                    }
                </fieldset>

                 <fieldset>
                    <legend>Pedido de Autorização , Divulgação e Veiculação - PADV</legend>
                    @if (Model?.BuscaEstadual?.PADV != null && Model.BuscaEstadual.PADV.Count != 0)
                    {
                        <table class="table table-striped table-hover">
                            <thead>
                                <tr>
                                    <th>Razão Social</th>
                                    <th>Agência</th>
                                    <th>CNPJ</th>
                                    <th>Numero PADV</th>
                                    <th>Orgao</th>
                                    <th>Valor Pago</th>
                                    <th>Ano</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var ambiental in Model.BuscaEstadual.PADV)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => ambiental.RazaoSocial)</td>
                                        <td>@Html.DisplayFor(m => ambiental.Agencia)</td>
                                        <td>@Html.DisplayFor(m => ambiental.CNPJExecutor)</td>
                                        <td>@Html.DisplayFor(m => ambiental.NumeroPADV)</td>
                                        <td>@Html.DisplayFor(m => ambiental.OrgaoPagador)</td>
                                        <td>@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", ambiental.ValorPago)</td>
                                        <td>@Html.DisplayFor(m => ambiental.Ano)</td>


                                    </tr>
                                }


                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhum PADV encontrada.</p>
                    }
                </fieldset>

                 <fieldset>
                    <legend>Acordos de Leniência</legend>
                    @if (Model?.BuscaFederal?.AcordosLeniencia != null && Model.BuscaFederal.AcordosLeniencia.Count != 0)
                    {
                        <table>
                            <thead>
                                <tr>
                                    <th>Identificaçao Acordo</th>
                                    <th>Numero Processo</th>
                                    <th>CNPJ</th>
                                    <th>Razão Social</th>
                                    <th>Inicio Acordo</th>
                                    <th>Fim Acordo</th>
                                    <th>Situação</th>
                                    <th>Efeitos</th>
                                    <th>Termos Acordos</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var federal in Model.BuscaFederal.AcordosLeniencia)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => federal.IdentificacaoAcordo)</td>
                                        <td>@Html.DisplayFor(m => federal.NumeroProcesso)</td>
                                        <td>@Html.DisplayFor(m => federal.CNPJ)</td>
                                        <td>@Html.DisplayFor(m => federal.RazaoSocial)</td>
                                        <td>@(federal.DataInicioAcordo.ToString("dd/MM/yyyy") ?? "-")</td>
                                        <td>@(federal.DataFimAcordo?.ToString("dd/MM/yyyy") ?? "-")</td>
                                        <td>@Html.DisplayFor(m => federal.Situacao)</td>
                                        <td>@Html.DisplayFor(m => federal.Efeitos)</td>
                                        <td>@Html.DisplayFor(m => federal.TermosAcordo)</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhum Acordo Encontrado</p>
                    }
                </fieldset>


                 <fieldset>
                    <legend>Aeronaves</legend>
                    @if (Model?.BuscaFederal?.Aeronave != null && Model.BuscaFederal.Aeronave.Count != 0)
                    {
                        <table>
                            <thead>
                                <tr>
                                    <th>Proprietário</th>
                                    <th>CPF</th>
                                    <th>Marca</th>
                                    <th>Operador</th>
                                    <th>CPFCGC</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var federal in Model.BuscaFederal.Aeronave)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => federal.Proprietario)</td>
                                        <td>@Html.DisplayFor(m => federal.CNPJCPF)</td>
                                        <td>@Html.DisplayFor(m => federal.Marca)</td>
                                        <td>@Html.DisplayFor(m => federal.Operador)</td>
                                        <td>@Html.DisplayFor(m => federal.CPFCGC)</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhuma Aeronave Encontrado</p>
                    }
                </fieldset>


                <fieldset>
                    <legend>Cadastro Nacional de Empresas Punidas - CNEP</legend>
                    @if (Model?.BuscaFederal?.Cnep != null && Model.BuscaFederal.Cnep.Count != 0)
                    {
                        <table>
                            <thead>
                                <tr>
                                    <th>Razão Social</th>
                                    <th>CNPJ</th>
                                    <th>Nro Processo</th>
                                    <th>Inicio Sanção</th>
                                    <th>Fim Sanção</th>
                                    <th>Valor Multa</th>
                                    <th>Fundamentação Legal</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var federal in Model.BuscaFederal.Cnep)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => federal.RazaoSocial)</td>
                                        <td>@Html.DisplayFor(m => federal.CNPJCPF)</td>
                                        <td>@Html.DisplayFor(m => federal.NroProcesso)</td>
                                        <td>@(federal.DataInicioSancao.ToString("dd/MM/yyyy") ?? "-")</td>
                                        <td>@(federal.DataFimSancao?.ToString("dd/MM/yyyy") ?? "-")</td>
                                        <td>@String.Format(new System.Globalization.CultureInfo("pt-BR"), "{0:C}", federal.ValorMulta)</td>
                                        <td>@Html.DisplayFor(m => federal.FundamentacaoLegal)</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhuma Empresa Encontrado</p>
                    }
                </fieldset>


                 <fieldset>
                    <legend>Cadastro Nacional de Empresas Privadas Sem Fins Lucrativo - CEPIM</legend>
                    @if (Model?.BuscaFederal?.Cepim != null && Model.BuscaFederal.Cepim.Count != 0)
                    {
                        <table>
                            <thead>
                                <tr>
                                    <th>Razão Social</th>
                                    <th>CNPJ</th>
                                    <th>Nro Convênio</th>
                                    <th>Orgão</th>
                                    <th>Impedimento</th>


                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var cepim in Model.BuscaFederal.Cepim)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => cepim.Nome)</td>
                                        <td>@Html.DisplayFor(m => cepim.CNPJ)</td>
                                        <td>@Html.DisplayFor(m => cepim.NroConvenio)</td>
                                        <td>@Html.DisplayFor(m => cepim.Orgao)</td>
                                        <td>@Html.DisplayFor(m => cepim.Impedimento)</td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhuma Empresa Encontrado</p>
                    }

                </fieldset>


                 <fieldset>
                    <legend>Cadastro Nacional de Empregadores de Trabalho Escravo</legend>
                    @if (Model?.BuscaFederal?.TrabalhoEscravo != null && Model.BuscaFederal.TrabalhoEscravo.Count != 0)
                    {
                        <table>
                            <thead>
                                <tr>
                                    <th>Empregador</th>
                                    <th>CPF</th>
                                    <th>Estabelecimento</th>
                                    <th>Ano</th>
                                    <th>UF</th>


                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var trabalho in Model.BuscaFederal.TrabalhoEscravo)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => trabalho.Empregador)</td>
                                        <td>@Html.DisplayFor(m => trabalho.CNPJCPF)</td>
                                        <td>@Html.DisplayFor(m => trabalho.Estabelecimento)</td>
                                        <td>@Html.DisplayFor(m => trabalho.Ano)</td>
                                        <td>@Html.DisplayFor(m => trabalho.UF)</td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhum Empresa Encontrada</p>
                    }
                </fieldset>


                 <fieldset>
                    <legend> TCE - CNPJ's Restritos '</legend>
                    @if (Model?.BuscaTCE?.CNPJRestrito != null && Model.BuscaTCE.CNPJRestrito.Count != 0)
                    {
                        <table>
                            <thead>
                                <tr>
                                    <th>Razão Social</th>
                                    <th>CNPJ</th>
                                    <th>Tipo Sanção</th>
                                    <th>Situação</th>
                                    <th>Inicio Sanção</th>
                                    <th>Fim Sanção</th>



                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var tce in Model.BuscaTCE.CNPJRestrito)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => tce.NomeRazaoSocial)</td>
                                        <td>@Html.DisplayFor(m => tce.CNPJCPF)</td>
                                        <td>@Html.DisplayFor(m => tce.TipoSancao)</td>
                                        <td>@Html.DisplayFor(m => tce.Situacao)</td>
                                        <td>@(tce.DataInicio.ToString("dd/MM/yyyy") ?? "-")</td>
                                        <td>@(tce.DataFim.ToString("dd/MM/yyyy") ?? "-")</td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    }
                    else
                    {
                        <p>Nenhum Registro Encontrado</p>
                    }
                </fieldset>


                 <fieldset>
                    <legend>TCU - Contas Irregulares</legend>
                    @if (Model?.BuscaTCU?.ContaIrregular != null && Model.BuscaTCU.ContaIrregular.Count != 0)
                    {
                        <table>
                            <thead>
                                <tr>
                                    <th>Razão Social</th>
                                    <th>CNPJ</th>
                                    <th>Municipio</th>
                                    <th>UF</th>
                                    <th>Deliberação</th>
